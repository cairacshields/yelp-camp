<% include partials/header %>


<div clas="container">

	<div class="row">
		<h1 style="text-align:center"> Sign up below! </h1>

			<form action="/register" method="POST">
				<div class="form-group col-xs-6 col-xs-offset-3">
					<input class="form-control centered" type="text" name="username" placeholder="Username">
				</div>
				<div class="form-group col-xs-6 col-xs-offset-3">
					<input class="form-control centered" type="password" name="password" placeholder="Password">
				</div>
				<div class="form-group col-xs-6 col-xs-offset-3">
					<input class="btn btn-primary" type="submit" value="Login">
				</div>
			</form>


	</div>

</div>








<% include partials/footer %>